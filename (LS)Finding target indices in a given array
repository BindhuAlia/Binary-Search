#include <iostream>

using namespace std;

int main()
{
    int a[10],f,l,m,t;
    int n,j=0;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    cout<<"enter target";
    cin>>t;
    while(j<n)
    {
        if (a[j]==t)
        {
            cout<<j<<' ';
        }
        j++;
    }
    return 0;
}
